    "lint": "ng lint",
    "e2e": "ng e2e",
    "ng-xi18n": "cd src && ../node_modules/.bin/ng-xi18n",
    "build-i18n": "for lang in en ; do ./node_modules/@angular/cli/bin/ng build -op dist/$lang --aot -prod --base-href /$lang/ --i18n-file=src/i18n/messages.$lang.xlf --i18n-format=xlf --locale=$lang; done",
    "postinstall": "echo there it goes!"
  },
