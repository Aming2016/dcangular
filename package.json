  "scripts": {
    "start": "ng serve --host 0.0.0.0",
    "start4202": "ng serve --port 4202",
    "start_env": "ng serve --host 0.0.0.0 --locale zh --environment ip ",
    "https": "ng serve --host 0.0.0.0 --locale zh --ssl 1 --ssl-key ssl/key.pem --ssl-cert ssl/server.crt",
